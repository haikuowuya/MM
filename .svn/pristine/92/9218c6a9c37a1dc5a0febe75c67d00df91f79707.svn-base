<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.maimengapp.hsh"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-sdk
        android:minSdkVersion="11"
        android:targetSdkVersion="23" />

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.READ_CONTACTS" />
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_logo"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme" >
        <activity
            android:name="com.maimengapp.hsh.MainActivity"
            android:label="@string/app_name"
            android:launchMode="singleTop" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.maimengapp.hsh.LoginActivity"
            android:label="@string/activity_login" >
        </activity>
        <activity
            android:name="com.maimengapp.hsh.RechargeActivity"
            android:label="@string/activity_recharge" />
        <activity
            android:name="com.maimengapp.hsh.CommunityActivity"
            android:label="@string/activity_community" />
        <activity
            android:name="com.maimengapp.hsh.GoodsActivity"
            android:label="@string/activity_goods" />
        <activity
            android:name="com.maimengapp.hsh.AboutActivity"
            android:label="@string/activity_about" />
        <activity
            android:name="com.maimengapp.hsh.PayActivity"
            android:label="@string/activity_pay" />
        <activity
            android:name="com.maimengapp.hsh.GoodsDetailActivity"
            android:label="@string/activity_goods_detail" />
        <activity
            android:name="com.maimengapp.hsh.SearchActivity"
            android:label="@string/activity_search" />
        <activity
            android:name="com.maimengapp.hsh.PostDetailActivity"
            android:label="@string/activity_post_detail" />
        <activity
            android:name="com.maimengapp.hsh.ActivityDetailActivity"
            android:label="@string/activity_activity_detail" />
        <activity
            android:name="com.maimengapp.hsh.ConfirmPayActivity"
            android:label="@string/activity_confirm_pay" />
        <activity
            android:name="com.maimengapp.hsh.PhoneActivity"
            android:label="@string/activity_free_phone" />
        <activity
            android:name="com.maimengapp.hsh.ContactActivity"
            android:label="@string/activity_contact" />
    </application>

</manifest>