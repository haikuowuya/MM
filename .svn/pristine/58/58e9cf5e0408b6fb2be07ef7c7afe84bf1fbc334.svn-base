<?xml version="1.0" encoding="utf-8"?>
<manifest
    package="com.maimengapp.hsh"
    xmlns:android="http://schemas.android.com/apk/res/android">


    <uses-permission android:name="android.permission.INTERNET"/>
    <uses-permission android:name="android.permission.READ_CONTACTS"/>
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE"/>

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_logo"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".MainActivity"
            android:label="@string/app_name"
            android:launchMode="singleTop">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>
                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
        </activity>
        <activity
            android:name=".LoginActivity"
            android:label="@string/activity_login">

        </activity>
        <activity
            android:name=".RechargeActivity"
            android:label="@string/activity_recharge"/>
        <activity
            android:name=".CommunityActivity"
            android:label="@string/activity_community"/>
        <activity
            android:name=".GoodsActivity"
            android:label="@string/activity_goods"/>
        <activity
            android:name=".AboutActivity"
            android:label="@string/activity_about"/>
        <activity
            android:name=".PayActivity"
            android:label="@string/activity_pay"/>
        <activity
            android:name=".GoodsDetailActivity"
            android:label="@string/activity_goods_detail"/>
        <activity
            android:name=".SearchActivity"
            android:label="@string/activity_search"/>
        <activity
            android:name=".PostDetailActivity"
            android:label="@string/activity_post_detail"/>
        <activity
            android:name=".ActivityDetailActivity"
            android:label="@string/activity_activity_detail"/>
        <activity
            android:name=".ConfirmPayActivity"
            android:label="@string/activity_confirm_pay"/>
        <activity
            android:name=".PhoneActivity"
            android:label="@string/activity_free_phone"/>
        <activity
            android:name=".ContactActivity"
            android:label="@string/activity_contact"/>
    </application>

</manifest>
